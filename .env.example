# Algolia Configuration
# Index naming convention: machina_XXX

# Application ID
ALGOLIA_APP_ID=latency

# Admin API Key (for indexing - keep secret, never commit)
ALGOLIA_ADMIN_KEY=

# Search API Key (for client-side chat - safe to expose)
ALGOLIA_SEARCH_KEY=

# Agent ID (from Agent Studio)
ALGOLIA_AGENT_ID=

# Index name (default: machina_pdf)
ALGOLIA_INDEX=machina_pdf
